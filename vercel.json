{
  "version": 2,
  "builds": [
    {
      "src": "backend/fastapi/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/*.py": {
      "includeFiles": "backend/fastapi/data/github-mauropelucchi-tedx_dataset-update_2024-details.csv"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/fastapi/api/index.py"
    }
  ]
}
